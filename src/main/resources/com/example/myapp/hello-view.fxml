<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="238.0" prefWidth="295.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.myapp.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Accordion />
   <Label fx:id="welcomeText1" prefHeight="17.0" prefWidth="102.0" text="Welcome Admin!">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Label>
   <TextArea id="User" prefHeight="2.0" prefWidth="257.0" promptText="Username" />
   <TextArea prefHeight="200.0" prefWidth="200.0" promptText="Password" />
   <Label fx:id="welcomeText11" prefHeight="17.0" prefWidth="45.0" text="Register" textFill="#44909e">
      <font>
         <Font name="System Italic" size="12.0" />
      </font>
   </Label>
    <Button onAction="#onHelloButtonClick" prefHeight="50.0" prefWidth="68.0" text="LOGIN" />
</VBox>
